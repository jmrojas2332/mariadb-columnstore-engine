[Unit]
Description=MariaDB columnstore engine DMLProc

# restart/stop dmlproc on restart/stop of writeengineserver
PartOf=mcs-writeengineserver.service
After=mcs-writeengineserver.service

[Service]
Type=simple

User=mysql
Group=mysql
LimitNOFILE=65536
LimitNPROC=65536

ExecStart=@ENGINE_BINDIR@/DMLProc

Restart=on-failure
TimeoutStopSec=5
