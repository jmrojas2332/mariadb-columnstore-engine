[Unit]
Description=MariaDB columnstore engine DDLProc

# restart/start ddlproc on restart/start of writeengineserver
PartOf=mcs-writeengineserver.service
After=mcs-dmlproc.service

[Service]
Type=simple

User=mysql
Group=mysql
LimitNOFILE=65536
LimitNPROC=65536

ExecStart=@ENGINE_BINDIR@/DDLProc

Restart=on-failure
TimeoutStopSec=5
